[application]

config/name="MergeGame"
run/main_scene="res://scenes/Chapter1.tscn"
config/icon="res://icon.png"

[autoload]

PlayerParametersObserver="*res://scripts/PlayerParametersObserver.gd"
MainMusicPlayer="*res://scripts/MainMusicPlayer.gd"
G="*res://ar-engine-sdk/global.gd"
WS="*res://ar-engine-sdk/websocket.gd"
IC="*res://ar-engine-sdk/input_controller.gd"
GridObserver="*res://scripts/GridObserver.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/fullscreen=true
window/size/test_width=960
